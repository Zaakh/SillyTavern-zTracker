{
    "name": "sillytavern-ztracker",
    "version": "1.0.1",
    "description": "SillyTavern zTracker extension.",
    "type": "module",
    "main": "src/scripts/main.js",
    "scripts": {
        "sync-version": "node scripts/sync-version.mjs",
        "copy-templates": "node scripts/copy-templates.mjs",
        "predev": "npm run sync-version && npm run copy-templates",
        "dev": "cross-env NODE_ENV=development sass src/styles/main.scss dist/style.css --source-map & cross-env NODE_ENV=development webpack --mode development --config webpack.config.cjs --watch",
        "prebuild": "npm run sync-version && npm run copy-templates",
        "build": "cross-env NODE_ENV=production sass src/styles/main.scss dist/style.css --no-source-map && cross-env NODE_ENV=production webpack --mode production --config webpack.config.cjs",
        "pretest": "npm run sync-version",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "check-version": "npm run sync-version -- --check",
        "version": "npm run sync-version",
        "prettify": "prettier --write \"src/**/*.ts\" \"templates/**/*.html\" \"src/**/*.tsx\""
    },
    "keywords": [],
    "author": "Zaakh",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@babel/preset-env": "^7.24.0",
        "@babel/preset-react": "^7.27.1",
        "@babel/preset-typescript": "^7.26.0",
        "@types/jest": "^29.5.14",
        "@types/jquery": "^3.5.32",
        "@types/node": "^22.13.1",
        "@types/react": "^19.1.11",
        "@types/react-dom": "^19.1.8",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "cross-env": "^7.0.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^30.2.0",
        "jquery": "^3.7.1",
        "prettier": "^3.4.2",
        "sass": "^1.83.4",
        "terser-webpack-plugin": "^5.3.10",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.2",
        "typescript": "^5.7.3",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "fast-xml-parser": "^5.2.5",
        "handlebars": "^4.7.8",
        "sillytavern-utils-lib": "^1.0.64"
    },
    "peerDependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    }
}
